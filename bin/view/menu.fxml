<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>

<MenuBar xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<menus>
		<Menu mnemonicParsing="false" text="File">
			<items>
				<MenuItem fx:id="login" mnemonicParsing="false" onAction="#handleMenuAction"
					text="Login" />
				<MenuItem fx:id="logout" mnemonicParsing="false" onAction="#handleMenuAction"
					text="Logout" />	
				<MenuItem fx:id="quit" mnemonicParsing="false" onAction="#handleMenuAction"
					text="Quit" />
			</items>
		</Menu>
		<Menu mnemonicParsing="false" text="Lists">
			<items>
				<MenuItem fx:id="authorList" mnemonicParsing="false"
					onAction="#handleMenuAction" text="Author List" />
				<MenuItem fx:id="authorAdd" mnemonicParsing="false"
					onAction="#handleMenuAction" text="Add Author" />
				<MenuItem fx:id="bookList" mnemonicParsing="false"
					onAction="#handleMenuAction" text="Book List" />
				<MenuItem fx:id="bookAdd" mnemonicParsing="false"
					onAction="#handleMenuAction" text="Add book" />	
				<MenuItem fx:id="pubGen" mnemonicParsing="false"
					onAction="#handleMenuAction" text="Publisher Reports" />
				 <!-- <MenuItem fx:id="ten" mnemonicParsing="false"
					onAction="#handleMenuAction" text="10000" /> -->	 	
			</items>
		</Menu>
	</menus>
</MenuBar>
